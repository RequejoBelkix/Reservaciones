@model Model.Usuario




@{
    ViewBag.Title = "Detalle";
}
<link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>

<!------ Include the above in your HEAD tag ---------->

<div class="container">
    <div class="row profile">
        <div class="col-md-3">
            <div class="profile-sidebar">
                <!-- SIDEBAR USERPIC -->

                <div class="profile-userpic">
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTc75m5FWG8jh8gSXJvoLkBJyZ7kpxivbtp9kKm-WKMxhg13BReLw" class="img-responsive" alt="">
                </div>
                <!-- END SIDEBAR USERPIC -->
                <!-- SIDEBAR USER TITLE -->
                <div class="profile-usertitle">
                    <div class="profile-usertitle-name">
                        @if (Session["Id"] != null)
                        {
                            <h4> @Session["Email"].ToString()</h4>
                        }
                    </div>
                </div>
                <!-- END SIDEBAR USER TITLE -->
                <!-- SIDEBAR BUTTONS -->
                <div class="profile-userbuttons">

                    <a href="http://localhost:54857/Usuario/Logout" class="btn btn-success btn-sm">Cerrar Sesio</a>

                </div>
                <!-- END SIDEBAR BUTTONS -->
                <!-- SIDEBAR MENU -->
                <div class="profile-usermenu">
                    <ul class="nav">
                        <li class="active">
                            <a href="#">
                                <i class="glyphicon glyphicon-home"></i>
                                Overview
                            </a>
                        </li>
                        <li>
                            <a href="http://localhost:54857/Usuario/Index">
                                <i class="glyphicon glyphicon-user"></i>
                                Listado de Usuarios
                            </a>
                        </li>

                        <li>
                            <a href="http://localhost:54857/Reserva/CreateReserva">
                                <i class="	glyphicon glyphicon-plus"></i>
                                Reservar Lavoratorios
                            </a>
                        </li>

                        <li>
                            <a href="http://localhost:54857/Auditoria/Index">
                                <i class="glyphicon glyphicon-asterisk"></i>
                                Historial
                            </a>
                        </li>

                        <li>
                            <a href="http://localhost:54857/Auditoria/Administracion">
                                <i class="glyphicon glyphicon-cog"></i>
                                Administración
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- END MENU -->
            </div>
        </div>
        <div class="col-md-9">
            <div class="profile-content">
                <h2>Detalle</h2>

                <div>
                    <h4>Usuario</h4>
                    <hr />
                    <dl class="dl-horizontal">


                        <dt>
                            @Html.DisplayNameFor(model => model.Nombre)
                        </dt>




                        <dt>
                            @Html.DisplayNameFor(model => model.Apellido)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Apellido)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Genero)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Genero)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Ttoken)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Ttoken)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Email)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Email)
                        </dd>


                        <dt>
                            @Html.DisplayNameFor(model => model.Chat_id)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Chat_id)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Celular)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Celular)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.NombreEscuela)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.IdEscuela)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.Profesiones)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.IdProfesion)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.Direcciones)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Direccion)
                        </dd>





                    </dl>
                </div>
                <p>
                    @Html.ActionLink("Listado de usuarios", "Index")
                </p>



            </div>
        </div>
    </div>
</div>
<center>
    <strong>Developer by <a href="https://www.facebook.com/belkix.requejomicolta" target="_blank">Belkix</a></strong>
</center>
